jwt:
  issuer: ${JWT_ISSUER}
  private-key-path: ${JWT_PRIVATE_KEY_PATH}
  public-key-path: ${JWT_PUBLIC_KEY_PATH}

auth:
  clients:
    core:
      id: ${AUTH_CLIENT_CORE_ID}
      secret: ${AUTH_CLIENT_CORE_SECRET}

    extension:
      id: ${AUTH_CLIENT_EXTENSION_ID}
      secret: ${AUTH_CLIENT_EXTENSION_SECRET}